{
    "description": "UEFI firmware for i386, with Secure Boot and SMM",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "/work/dst/qemu-linux-x86_64-5.0.0-z2/share/qemu/edk2-i386-secure-code.fd",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "/work/dst/qemu-linux-x86_64-5.0.0-z2/share/qemu/edk2-i386-vars.fd",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "i386",
            "machines": [
                "pc-q35-*"
            ]
        }
    ],
    "features": [
        "acpi-s3",
        "requires-smm",
        "secure-boot",
        "verbose-dynamic"
    ],
    "tags": [

    ]
}
